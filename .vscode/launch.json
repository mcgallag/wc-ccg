{
  "version": "0.2.0",
  "configurations": [{
    "name": "Launch Firefox",
    "type": "firefox",
    "request": "launch",
    "reAttach": true,
    "url": "http://localhost:8080/index.html",
    "webRoot": "webpack:///",
    "suggestPathMappingWizard": true,
    "pathMappings": [{
      "url": "webpack:///src",
      "path": "${workspaceFolder}/src"
    }]
  }]
}